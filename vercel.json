{
  "version": 2,
  "buildCommand": "node scripts/eslint-check.js && next build",
  "ignoreCommand": "exit 1",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  "github": {
    "silent": false,
    "autoJobCancelation": true,
    "enabled": true
  }
} 