{
  "version": 2,
  "buildCommand": "node scripts/eslint-check.js && next build",
  "ignoreCommand": "exit 1",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": "out",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  "github": {
    "silent": false,
    "autoJobCancelation": true,
    "enabled": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ]
} 